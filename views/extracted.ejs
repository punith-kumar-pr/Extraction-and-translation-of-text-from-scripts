<%- include('includes/head.ejs') %>
    </head>

    <body>
        <%- include('includes/navbar.ejs') %>
            <!-- HEADING  -->
            <div class="container px-5 py-10 mx-auto">
                <div class="xl:w-1/2 lg:w-3/4 w-full mx-auto text-center">
                    <p class="leading-relaxed text-2xl text-red-600">
                        INSCRIPTIONS INFORMATION EXTRACTION USING OCR TECHNIQUE
                    </p>
                    <span class="inline-block h-1 w-10 rounded bg-indigo-500 mt-8 mb-6"></span>
                    <h2 class="text-gray-900 font-medium title-font tracking-wider text-lg">Furious Four</h2>
                    <p class="text-gray-500">From DSCE</p>
                </div>
            </div>
            <!-- END HEADING  -->


            <!-- IMAGE -->
            <div class="container px-5 py-1 mx-auto my-auto flex items-center sm:flex-row flex-col">
                <div class="px-5 py-3 items-center h-auto max-w-full mx-auto my-auto">

                    <img src="<%=img_url%>"id="output" width="100%" height="100%" />
                </div>
            </div>
            
            <!-- <script src="/javascript/img_upload_js.js"></script> -->


            <!-- END IMAGE-->


            <!-- DISPLAY ORIGINAL AND TRANSLATION -->
            <div class="container px-5 py-6 mx-auto flex items-center sm:flex-row flex-col">
                <div class="flex flex-wrap container">
                    <div class="w-full md:w-1/2 p-4 ">
                        <p class="text-center text-xl text-red-600">Text from the inscription/image uploaded</p> <br>
                        <p id ="kantext" class="text-lg text-gray-800 leading-relaxed">
                            <%= kan_txt  %>
                        </p>
                    </div>
                    <div class="w-full md:w-1/2 p-4">
                        <p class="text-center text-xl text-red-600 ">Transalation of it into English</p> <br>
                        <p class="text-lg text-gray-800 leading-relaxed mb-6">
                            <%= eng_txt %>
                        </p>
                    </div>
                </div>
            </div>
            <!-- END DISPLAY ORIGINAL AND TRANSLATION -->

            <!-- THANK YOU -->
            <div class="container px-5 py-5 mx-auto bg-blue-900">
                <div class="xl:w-1/2 lg:w-3/4 w-full mx-auto text-center">
                    <p class="leading-relaxed text-2xl text-red-600">
                        Hope this was Helpful
                    </p>
                </div>
            </div>
            <!-- END THANK YOU -->

            <%- include('includes/footer.ejs') %>
                <%- include('includes/end.ejs') %>